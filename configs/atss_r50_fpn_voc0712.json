{
    "data": {
	"train_ann":  "path_to_voc0712_trainval.json",
	"test_ann":   "path_to_voc2007_test.json",
	"train_imgs": "path_to_train_imgs",
	"test_imgs":  "path_to_test_imgs",
	"train_transforms": {"img_scale": [1000, 600],
			     "flip_ratio": 0.5,
			     "img_mean": [123.675, 116.28, 103.53],
			     "img_std":  [58.395, 57.12, 57.375],
			     "pad_divisor": 32},
	"test_transforms":  {"img_scale": [1000, 600],
			     "img_mean": [123.675, 116.28, 103.53],
			     "img_std":  [58.395, 57.12, 57.375],
			     "pad_divisor": 32},
	"train_workers": 4,
	"test_workers":  4},
    "model": {
	"backbone":  {"type": "resnet50",
		      "frozen_stages": 1,
		      "pretrained": "path_to_pretrained_ckpt_or_None"},
	"neck":      {"out_channels": 256,
		      "feat_channels": [256, 512, 1024, 2048],
		      "start_level": 1,
		      "num_outs": 5},
	"atss_head": {"in_channels": 256,
		      "stacked_convs": 4,
		      "feat_channels": 256,
		      "num_classes": 20,
		      "anchor_opts": {"anchor_scales": [8],
				      "anchor_ratios": [1.0],
				      "anchor_strides": [8, 16, 32, 64, 128],
				      "center_offset": 0.0},
		      "bbox_coder_opts": {"means": [0.0, 0.0, 0.0, 0.0],
					  "stds":  [0.1, 0.1, 0.2, 0.2]},
		      "loss_cls_opts": {"type": "FocalLoss", "gamma": 2.0, "alpha": 0.25, "loss_weight": 1.0},
		      "loss_bbox_opts": {"type": "GIoULoss", "loss_weight": 2.0},
		      "loss_centerness_opts": {"type": "BinaryCrossEntropyLoss", "loss_weight":1.0},
		      "train_opts": {"topk": 9},
		      "test_opts": {"nms_pre": 1000, "score_thr": 0.05, "nms_thr": 0.6, "max_per_img": 100}}},
    "optimizer": {"type": "SGD",
		  "lr": 0.000625,
		  "momentum": 0.9,
		  "weight_decay": 0.0001},
    "lr_opts":   {"decay_epochs": [9, 12],
		  "warmup_steps": 500,
		  "warmup_start": 0.001},
    "total_epochs": 12,
    "save_ckpt_period": 2,
    "log_period": 100
}

